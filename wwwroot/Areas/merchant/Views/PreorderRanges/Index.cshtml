@model IEnumerable<CommunityBackend.Models.PreorderRange>

@{
    Layout = "~/Areas/merchant/Views/Shared/_layoutForFrame.cshtml";
    ViewBag.Title = "预约时段管理";
}

<h2>预约时段目录</h2>

<p>
    @Html.ActionLink("创建", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RangeStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RangeEnd)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RangeStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RangeEnd)
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id = item.PreorderRangeID }) |
            @Html.ActionLink("删除", "Delete", new { id = item.PreorderRangeID })
        </td>
    </tr>
}

</table>
