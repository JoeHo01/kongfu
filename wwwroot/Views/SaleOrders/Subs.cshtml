@model IEnumerable<CommunityBackend.Models.SaleOrderDetail>

@{
    Layout = "~/Views/Shared/_layoutForFrame.cshtml"; 
    ViewBag.Title = "销售明细"; 
}

<h2>明细</h2>

<p></p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WareID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WareCnt)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ware.WareName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WareCnt)
        </td>
    </tr>
}

</table>

<p>
    @Html.ActionLink("返回目录", "Index")
</p>
