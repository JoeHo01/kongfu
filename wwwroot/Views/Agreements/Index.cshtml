@using CommunityBackend.Models;
@model IEnumerable<CommunityBackend.Models.Agreement>

@{
    Layout = "~/Views/Shared/_layoutForFrame.cshtml";
    ViewBag.Title = "协议管理";
}

<h2>目录</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AgreementType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.AgreementType.GetDisplayName()
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id = item.AgreementType }) |
            @Html.ActionLink("查看", "Details", new { id = item.AgreementType })
        </td>
    </tr>
}

</table>
