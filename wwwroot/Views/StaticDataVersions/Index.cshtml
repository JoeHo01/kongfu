@model IEnumerable<CommunityBackend.Models.StaticDataVersion>

@{
    Layout = "~/Views/Shared/_layoutForFrame.cshtml";
    ViewBag.Title = "数据版本管理";
}

<h2>数据版本</h2>

<p>
    @Html.ActionLink("创建", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VersionID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comment)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VersionID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comment)
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id = item.VersionID }) |
            @Html.ActionLink("查看", "Details", new { id = item.VersionID }) |
            @Html.ActionLink("删除", "Delete", new { id = item.VersionID })
        </td>
    </tr>
}

</table>
