@model IEnumerable<CommunityBackend.Models.PreInfoType>

@{
    Layout = "~/Views/Shared/_layoutForFrame.cshtml";
    ViewBag.Title = "预约类别管理";
}

<h2>预约类别目录</h2>

<h4>
    @Html.ActionLink("管理租赁类别", "Index", "InfoTypes")
</h4>

<p>
    @Html.ActionLink("创建", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PreInfoTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PreInfoTypeName)
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id = item.PreInfoTypeID }) |
            @Html.ActionLink("查看", "Details", new { id = item.PreInfoTypeID }) |
            @Html.ActionLink("删除", "Delete", new { id = item.PreInfoTypeID })
        </td>
    </tr>
}

</table>
